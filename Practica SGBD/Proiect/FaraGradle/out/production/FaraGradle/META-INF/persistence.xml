<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="PSGBD">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>control.AlbumsEntity</class>
        <class>control.FavoritesEntity</class>
        <class>control.FriendsEntity</class>
        <class>control.MembersEntity</class>
        <class>control.SongsEntity</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:oracle:thin:@localhost:1521:xe"/>
            <property name="eclipselink.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
            <property name="eclipselink.jdbc.user" value="system"/>
            <property name="eclipselink.jdbc.password" value="oracle"/>
        </properties>
    </persistence-unit>
</persistence>
